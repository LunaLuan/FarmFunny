#ifndef RESUORCEMANAGER_H_
#define RESUORCEMANAGER_H_

class ResuorceManager {
public:
	ResuorceManager();

	void buyFood(int m){
		food=food+m*10;
		money=money-m;
	}
	virtual ~ResuorceManager();

protected:
	int food,money;


};



#endif /* RESUORCEMANAGER_H_ */
