#ifndef CAT_H_
#define CAT_H_

#include "EmotionAnimal.h"

class Cat: public EmotionAnimal {
public:
	Cat();



	virtual void sound() = 0;
	virtual void eat() = 0;
	virtual void goOut() = 0;
	virtual void goBack() = 0;
	virtual void die() = 0;
	virtual void reproduce() = 0;

	virtual ~Cat();
};

#endif /* CAT_H_ */
