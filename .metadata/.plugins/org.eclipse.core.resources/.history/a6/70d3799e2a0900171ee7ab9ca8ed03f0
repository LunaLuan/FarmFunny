#ifndef CHICKKEN_H_
#define CHICKKEN_H_

#include "HappyAnimal.h"

class Chickken: public HappyAnimal {
public:
	Chickken();
	
	virtual void sound();
	virtual void eat();
	virtual void goOut();
	virtual void goBack();
	virtual void die();
	virtual void reproduce();


	virtual ~Chickken();
};

#endif /* CHICKKEN_H_ */
