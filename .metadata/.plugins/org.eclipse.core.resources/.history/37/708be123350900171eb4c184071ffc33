#ifndef CHICKKEN_H_
#define CHICKKEN_H_


#include "EmotionAnimal.h"
#include <iostream>
#include <cstdlib>
using namespace std;

class Chickken: public EmotionAnimal {
public:
	Chickken();
	
	virtual void sound();
	virtual void eat();
	virtual void goOut();
	virtual void goBack();
	virtual void die();
	virtual int reproduce();


	virtual ~Chickken();
};

#endif /* CHICKKEN_H_ */
